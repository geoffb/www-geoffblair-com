{
  "name": "www-geoffblair-com",
  "private": true,
  "scripts": {
    "build": "node build.js",
    "clean": "rm -rf _site",
    "deploy": "aws s3 sync _site s3://www.geoffblair.com --cache-control max-age=86400 --size-only --acl public-read --delete --profile geoffblair",
    "develop": "node build.js --watch --serve --drafts"
  },
  "devDependencies": {
    "jade": "^1.3.1",
    "markdown-js": "0.0.3",
    "metalsmith": "^0.7.0",
    "metalsmith-collections": "^0.6.0",
    "metalsmith-dev": "0.0.1",
    "metalsmith-drafts": "0.0.1",
    "metalsmith-ignore": "^0.1.2",
    "metalsmith-less": "^0.1.1",
    "metalsmith-markdown": "^0.2.1",
    "metalsmith-metallic": "^0.3.1",
    "metalsmith-permalinks": "^0.3.0",
    "metalsmith-tags": "^1.2.1",
    "metalsmith-templates": "^0.5.0",
    "minimist": "0.0.8"
  }
}
